{
    "manifest_version": 3,
    "name": "7tv-youtube",
    "version": "1.0",
    "description": "7tv emotes for youtube comments",
    "permissions": ["activeTab"],
    "host_permissions": ["https://www.youtube.com/*"],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }